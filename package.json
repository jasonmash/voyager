{
  "name": "voyager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --reporter=mocha-junit-reporter --reporter-options mochaFile=results/test-unit.xml,toConsole=true",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve",
    "test:ci": "vue-cli-service test:e2e --headless --reporter mocha-junit-reporter --reporter-options mochaFile=results/test-e2e-[hash].xml,toConsole=true"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@types/downloadjs": "^1.4.1",
    "@types/lodash": "^4.14.149",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.14.0",
    "core-js": "^3.6.4",
    "downloadjs": "^1.4.7",
    "echarts": "^4.7.0",
    "echarts-gl": "^1.1.1",
    "papaparse": "^5.3.0",
    "shvl": "^1.3.1",
    "vue": "^2.6.11",
    "vue-class-component": "^6.0.0",
    "vue-echarts": "^4.1.0",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.1.6",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-e2e-cypress": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-plugin-unit-mocha": "~4.3.0",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^1.0.0-beta.32",
    "chai": "^4.1.2",
    "lint-staged": "^7.2.2",
    "mocha-junit-reporter": "^1.23.3",
    "typescript": "~3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
